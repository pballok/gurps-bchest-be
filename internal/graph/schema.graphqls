type Character {
  name: String!
  attributes: [Attribute!]!
}

enum AttributeType {
  ST,
  DX,
  IQ,
  HT,
  HP,
  WILL,
  PER,
  FP,
  BS,
  BM,
}

type Attribute {
  attributeType: AttributeType!
  value: Int!
  cost: Int!
}

input CharacterImport {
  data: String!
}

type Query {
  character(name: String!): Character!
}

type Mutation {
  importCharacter(input: CharacterImport!): Character!
}
